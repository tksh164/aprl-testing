            "name": "[concat('blockblobprmzrs', variables('uniqueSuffix'))]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2023-01-01",
            "location": "[variables('location')]",
            "kind": "BlockBlobStorage",
            "sku": {
                "name": "Premium_ZRS"
            },
            "properties": {
            }
        },

        //
        // File Premium
        //

        // File Premium LRS
        {
            "name": "[concat('fileprmlrs', variables('uniqueSuffix'))]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2023-01-01",
            "location": "[variables('location')]",
            "kind": "FileStorage",
            "sku": {
                "name": "Premium_LRS"
            },
            "properties": {
            }
        },

        // File Premium ZRS
        {
            "name": "[concat('fileprmzrs', variables('uniqueSuffix'))]",
            "type": "Microsoft.Storage/storageAccounts",
            "apiVersion": "2023-01-01",
            "location": "[variables('location')]",
            "kind": "FileStorage",
            "sku": {
                "name": "Premium_ZRS"
            },
            "properties": {
            }
        }
    ]
}
